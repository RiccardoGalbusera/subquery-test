    "@subql/cli": "latest",
    "@subql/node": "latest",
    "@subql/node-ethereum": "latest",
    "@subql/testing": "latest",
    "@subql/types": "latest",
    "ethers": "^5.7.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/pg": "^8.11.6",
    "assert": "^2.0.0",
    "aws-sdk": "^2.1625.0",
    "dotenv": "^16.4.5",
    "pg": "^8.11.5"
  }
